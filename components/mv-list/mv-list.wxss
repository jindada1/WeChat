/* components/mv-list.wxss */

.no-wrap-text {
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  font-size: 14px;
}

.sub-text {
  font-size: 12px !important;
  color: gray;
}

.hoverd {
  background-color: whitesmoke;
}

.contain {
  height: 100%;
  width: 100%;
  display: flex;
  position: absolute;
  align-items: center;
  justify-content: center;
  font-size: 14px;
}